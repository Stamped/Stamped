<script type="text/javascript">
      var pre_val = $("#sms_content").val();
      function updateSMS() {
        if ($.trim($('input#send_from_name').val()).length > 0) {
            $("#sms_content").val("Hey, it's " + $.trim($('input#send_from_name').val()) + ". " + pre_val);
        }
        else {
            $("#sms_content").val(pre_val);
        }
      }
      $(document).ready(function($) {
        /** Share functionality */
        $('document').click(function() {
          $('.sd').hide();
        });
        $(".sd").click(function(e) {
          e.stopPropagation();
        });
        $('.sb').click(function(){
          $('.sd').hide();
          $(this).next().show();
          return false;
        });
        $('.pop_close').click(function(){
          $('.sd').hide();
        });
        $('#social_twitter a').click(function(){
            _gaq.push(['_trackEvent', 'Menu', 'Share This', 'Twitter']);
        });
        $('#share_email_btn').click(function(){
            _gaq.push(['_trackEvent', 'Menu', 'Share This', 'Email']);
        });
        $('#share_sms_btn').click(function(){
            $('span.charleft').html((140-$('.max140').val().length) + ' Characters Left');
            _gaq.push(['_trackEvent', 'Menu', 'Share This', 'Text']);
        });
        $('#form_send_sms').submit(function() {
          var send_to_sms = $('#send_to_sms').val();
          var sms_content = $('#sms_content').val();
          if ($.trim(send_to_sms).length == 0) {
            alert ('Please enter phone number(s).');
            return false;
          }
          if ($.trim(sms_content).length > 140) {
            alert('Your sms message must be shorter than 140 characters.');
            return false;
          }
        });
        $('#form_send_email').submit(function() {
          var send_to_email = $('#send_to_email').val();
          var send_from_email = $('#send_from_email').val();
          if ($.trim(send_to_email).length == 0) {
            alert ('Please enter email(s) to send the message to.');
            return false;
          }
          if ($.trim(send_from_email).length == 0) {
            alert('Please enter the email address used to send the messages.');
            return false;
          }
        });
        $('.placeholder').live('focus', function(){
           $(this).removeClass('placeholder').val('');
        });
        $('.hasplaceholder').live('blur', function(){
           if ($(this).val() == '') {
               $(this).addClass('placeholder').val("Email Address");
           }
        });
        /** // Share functionality */
        $('textarea.max140').live('keyup', function() {
            if ($(this).val().length > 140) {
                $(this).val($(this).val().substring(0,140)) 
            }
            $(this).parent().children('span.charleft').html((140-$(this).val().length) + ' Characters Left');
        });
        /** // SMS send from name functionality */
        $('input#send_from_name').live('keyup', function() {
            updateSMS();
        });
        $('input#send_from_name').live('blur', function() {
            updateSMS();
        });

        var didscroll = 0;
        var elemTop;
        var elemBottom;
        $('#single_arrow').addClass('fixedposition').hide();

        $("#single_arrow").live('click', function(){
          $('html, body').animate({scrollTop: '0px'}, 800);
        });
        $(window).scroll(function() {
          if (didscroll == 0 ) {
                didscroll = 1;
                elemTop = $('#single_arrow').offset().top;
                elemBottom = elemTop + $('#single_arrow').height();
          }
          if (isScrolledIntoView('#single_arrow') == true)
                $('#single_arrow').fadeIn();
          else
                $('#single_arrow').fadeOut();
          });

          function isScrolledIntoView(elem)
          {
              var docViewTop = $(window).scrollTop();
              var docViewBottom = docViewTop + $(window).height();

              return (docViewTop > 0 && docViewBottom > ($('#rightcol').height() + $('#sp_mainWrapper').height() + 56) );
          }
      });
</script>
